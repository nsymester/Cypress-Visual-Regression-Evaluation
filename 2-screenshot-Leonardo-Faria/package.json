{
	"name": "cypress-test-image",
	"version": "1.0.0",
	"description": "Repository showing Cypress tests",
	"main": "index.js",
	"scripts": {
		"serve": "serve public -p 8000",
		"cy:run": "cypress run --config-file cypress-base.json",
		"cy:open": "cypress open --config-file cypress-base.json",
		"cy:doc": "set CYPRESS_updateSnapshots=false && cmd /k cypress-script.bat $CYPRESS_updateSnapshots"
	},
	"keywords": [
		"cypress"
	],
	"author": "Leonardo Faria",
	"license": "MIT",
	"devDependencies": {
		"cypress": "^5.4.0",
		"cypress-image-snapshot": "^3.1.1",
		"serve": "^11.3.2"
	},
	"dependencies": {
		"cross-env": "^7.0.2"
	}
}
